{
  "name": "AchievementBadges",
  "version": "0.0.1",
  "author": ["Lens"],
  "url": "https://github.com/femiwiki/AchievementBadges",
  "descriptionmsg": "achievementbadges-desc",
  "license-name": "GPL-2.0-or-later",
  "type": "specialpage",
  "requires": {
    "MediaWiki": ">= 1.35.0",
    "extensions": {
      "Echo": "*"
    }
  },
  "AutoloadNamespaces": {
    "MediaWiki\\Extension\\AchievementBadges\\": "includes/"
  },
  "ResourceFileModulePaths": {
    "localBasePath": "modules",
    "remoteExtPath": "AchievementBadges/modules"
  },
  "ResourceModules": {
    "ext.achievementbadges.special.achievements.styles": {
      "styles": ["styles/spacial-achievements.less"]
    }
  },
  "Hooks": {
    "AddNewAccount": "main",
    "BeforeCreateAchievement": "MediaWiki\\Extension\\AchievementBadges\\HookHandler::onBeforeCreateAchievement",
    "BeforeCreateEchoEvent": "MediaWiki\\Extension\\AchievementBadges\\HookHandler::onBeforeCreateEchoEvent",
    "GetBetaFeaturePreferences": "MediaWiki\\Extension\\AchievementBadges\\HookHandler::onGetBetaFeaturePreferences",
    "UserSaveSettings": "main"
  },
  "HookHandlers": {
    "main": {
      "class": "MediaWiki\\Extension\\AchievementBadges\\HookHandler",
      "services": ["MainConfig"]
    }
  },
  "SpecialPages": {
    "Achievements": "MediaWiki\\Extension\\AchievementBadges\\SpecialAchievements"
  },
  "config": {
    "AchievementBadgesEnableBetaFeature": {
      "value": false
    }
  },
  "MessagesDirs": {
    "AchievementBadges": ["i18n"]
  },
  "ExtensionMessagesFiles": {
    "AchievementBadgesAlias": "AchievementBadges.i18n.alias.php"
  },
  "manifest_version": 2
}
